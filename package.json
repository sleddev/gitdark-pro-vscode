{
	"name": "gitdark-pro",
	"displayName": "GitDark Pro theme",
	"description": "GitHub theme x One Dark Pro",
	"version": "1.0.1",
	"publisher": "sleddev",
	"license": "MIT",
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#1b1f23",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sleddev/gitdark-pro-vscode"
	},
	"bugs": {
		"url": "https://github.com/sleddev/gitdark-pro-vscode/issues"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"theme",
		"github",
		"light",
		"dark",
		"protanopia",
		"colorblind",
		"one dark pro",
		"gitdark"
	],
	"contributes": {
		"themes": [
			{
				"label": "GitDark Pro",
				"uiTheme": "vs-dark",
				"path": "./themes/gitdark-pro.json"
			}
		]
	},
	"devDependencies": {
		"@vscode/vsce": "^3.2.2"
	},
	"scripts": {
		"package": "vsce package -o ./build/gitdark-pro-vscode-theme.vsix"
	},
	"publishConfig": {
		"access": "public"
